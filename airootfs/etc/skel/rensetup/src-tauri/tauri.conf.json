{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rensetup",
  "version": "0.1.0",
  "identifier": "net.ren-net.rensetup",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "RenOS setup utility",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
